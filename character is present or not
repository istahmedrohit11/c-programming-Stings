// Check if a given character is present in a string or not.
#include<stdio.h>
void cheakChar(char str[],char ch){
    for(int i=0; str[i]!='\0' ;i++){
        if(str[i]==ch){
            printf("Character is present");
            return;
        }
    }
    printf("Character is not present");
}
int main(){
    char str[]="Rohit";
    char ch='h';
    cheakChar(str,ch);
    return 0;
}
