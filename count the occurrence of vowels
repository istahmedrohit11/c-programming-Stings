//Write a function to count the occurrence of vowels in a string.
#include<stdio.h>
int countVowles(char str[]){
    int count =0;
    for(int i=0;str[i]!='\0';i++){
        if( str[i]=='a' ||str[i]=='e' ||str[i]=='i' ||str[i]=='o' ||str[i]=='u' ){
            count++;
        }
    }
    return count;
}
int main(){
        char str[]="Rohit";
        printf("vowles are: %d",countVowles(str));
        return 0;
}
